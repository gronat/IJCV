@incollection{Caruana98,
  title = "A Dozen Tricks with Multitask Learning",
  author = "Rich Caruana",
  booktitle = "Neural Networks: Tricks of the Trade",
  editor = "G.B. Orr and K.-R. Mueller",
  year =1998,
  publisher = "Springer-Verlag",
  url = "http://www.informatik.uni-trier.de/~ley/db/conf/nips/nips1996.html",
 abstract = "Multitask Learning is an inductive transfer method that
         improves generalization accuracy on a main task by using the
         information contained in the training signals of other related
         tasks. It does this by learning the extra tasks in parallel with
         the main task while using a shared representation; what is
         learned for each task can help other tasks be learned better.
         This chapter describes a dozen opportunities for applying
         multitask learning in real problems. At the end of the chapter
         we also make several suggestions for how to get the most our
         of multitask learning on real-world problems."
}


